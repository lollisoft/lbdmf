void LB_STDCALL lbDatabaseReport::init(char* SQLString, char* DBName, char* DBUser, char* DBPass) {
	_CL_LOG << \"lbDatabaseReport::init('\" << 
			SQLString << \"', '\" << 
			DBName << \"', '\" << 
